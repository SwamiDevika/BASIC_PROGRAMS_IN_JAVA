import java.util.*;
public class MyClass
{
    public static void main(String args[])
    {
        Scanner scan= new Scanner(System.in);
        
        int n1= scan.nextInt();
        //int n2= scan.nextInt();
        
      int a=0,b=0,i=0;
      while(i<2)
      {
         a= n1%10;
         b+=a;
         n1=n1/10;
         i++;
      }
       System.out.print(b);
    }
}
/*
123456
11*/
